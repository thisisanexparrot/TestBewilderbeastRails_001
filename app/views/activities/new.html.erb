<div class="header">
  <div class="container">
    <h1>Messenger</h1>
  </div>
</div>


<h1>New activity</h1>
<%= form_for(@activity) do |f| %>
	<div class="field">
		<%= f.label :message %><br>
		<%= f.text_area :activity_name %>
	</div>
	<div class="actions">
		<%= f.submit "Create" %>
	</div>
<% end %>